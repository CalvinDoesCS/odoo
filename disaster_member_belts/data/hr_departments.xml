<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <!-- ============================================================
         HR Departments for Dojo Members and Instructors.
         noupdate="1" so they are only created, never overwritten.
    ============================================================ -->

    <record id="hr_dept_students" model="hr.department">
        <field name="name">Students</field>
    </record>

    <record id="hr_dept_instructors" model="hr.department">
        <field name="name">Instructors</field>
    </record>

</odoo>

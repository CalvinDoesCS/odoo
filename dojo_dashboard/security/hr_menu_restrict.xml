<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--
        Restrict the HR "Employees" root menu so that only Dojo Managers
        (and Odoo Administrators) can see it.
        Instructors will NOT see the Employees app.
    -->
    <record id="hr.menu_hr_root" model="ir.ui.menu">
        <field name="group_ids" eval="[
            (4, ref('dojo_dashboard.group_dojo_manager')),
            (4, ref('base.group_system')),
        ]"/>
    </record>
</odoo>
